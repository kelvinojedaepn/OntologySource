    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/kevin/ontologies/2023/10/OntologiaLOPDP#autoridad_de_proteccion_de_datos_personales -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/kevin/ontologies/2023/10/OntologiaLOPDP#autoridad_de_proteccion_de_datos_personales">
