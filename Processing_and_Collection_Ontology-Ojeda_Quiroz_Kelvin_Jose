    </DataPropertyAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="#cuáles_serán_las_medidas_correctivas"/>
        <NamedIndividual IRI="#medidas_correctivas"/>
        <Literal>La imposición de medidas técnicas, jurídicas, organizativas o administrativas a garantizar un tratamiento adecuado de datos personales</Literal>
    </DataPropertyAssertion>
